@model PageUp.People.Admin.ViewModels.HiringManagerDashboard.HiringManagerDashboardViewModel
@{
    Layout = "~/Views/Shared/_GelLayout.cshtml";
    ViewBag.Title = @Translator["HIRING_MANAGER_DASHBOARD"];
    ViewBag.SubTitle = Model.SubTitle;
    ViewBag.FixedWidth = true;
}

<div class="row">
    @{
        var showGuidlines = !String.IsNullOrWhiteSpace(@Model.GuidelinesContent);
        const string threeCardLayoutColumnsClass = "col-md-6 col-lg-4 mb-4";
        const string fourCardLayoutColumnsClass = "col-md-6 col-lg-4 col-xl-3 mb-4";

        <script src="https://cdnjs.cloudflare.com/ajax/libs/bodymovin/5.12.2/lottie.min.js"></script>

        if (showGuidlines)
        {
            <div class="col-xl-9">
                <div class="row">
                    @foreach (var card in Model.Cards)
                    {
                        card.ColumnClass = threeCardLayoutColumnsClass;
                        @Html.Partial("~/Views/HiringManagerDashboard/OptionCard.cshtml", card)
                    }
                </div>
            </div>

            <div class="col-xl-3">
                <h3>@Translator["HIRING_MANAGER_DASHBOARD_GUIDELINES"]</h3>
                @Html.Raw(@Model.GuidelinesContent)
            </div>
        }
        else
        {
            foreach (var card in Model.Cards)
            {
                card.ColumnClass = fourCardLayoutColumnsClass;
                @Html.Partial("~/Views/HiringManagerDashboard/OptionCard.cshtml", card)
            }
        }
    }

</div>
